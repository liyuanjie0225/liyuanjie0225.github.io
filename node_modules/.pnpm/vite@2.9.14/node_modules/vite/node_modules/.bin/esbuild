#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/liyuanjie/Documents/学习/vitepress-starter/liyuanjie0225.github.io/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/liyuanjie/Documents/学习/vitepress-starter/liyuanjie0225.github.io/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../esbuild@0.14.53/node_modules/esbuild/bin/esbuild" "$@"
else
  exec node  "$basedir/../../../../../esbuild@0.14.53/node_modules/esbuild/bin/esbuild" "$@"
fi
